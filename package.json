{
  "name": "whylabs-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "lint": "remark docs --frail",
    "start": "docusaurus start",
    "doc-build": "docusaurus build",
    "copy-docs": "mkdir _docs && mv build/* _docs/ && mv _docs build/ && mv build/_docs build/docs",
    "build-and-copy": "yarn doc-build && yarn copy-docs",
    "create-netlify-redirects": "echo \"/ /docs\" > build/_redirects",
    "build": "yarn lint && yarn build-and-copy && yarn create-netlify-redirects",
    "swizzle": "docusaurus swizzle",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear",
    "deploy:preview": "rm -fr build && yarn build && netlify deploy --dir build/",
    "deploy:beta": "yarn clear && yarn build && netlify deploy --dir build/ --prod",
    "deploy:prod": "yarn clear && yarn build && netlify deploy --dir build/ --prod"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.70",
    "@docusaurus/plugin-google-analytics": "^2.0.0-alpha.70",
    "@docusaurus/preset-classic": "2.0.0-alpha.70",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "netlify-cli": "^3.10.6",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  }
}
